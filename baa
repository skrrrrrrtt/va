-- PLS DONATE: Auto Booth + Anti-AFK + Auto Jump (NO CHAT)
-- Uses correct booth-claiming remotes

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local VirtualUser = game:GetService("VirtualUser")
local LocalPlayer = Players.LocalPlayer

local ClaimBoothRemote = ReplicatedStorage.Remotes["49EVHLTp6L9az4SnjlAhul4jOFY="]
local TeleportRemote   = ReplicatedStorage.Remotes["djuE5GPqBAMl0VnVHoJcE7nZUTM="]

-- =====================
-- ANTI-AFK
-- =====================
task.spawn(function()
    while task.wait(240) do
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
end)

-- =====================
-- AUTO CLAIM BOOTH
-- =====================
local function autoClaim()
    task.wait(2)

    -- Request a booth from server
    ClaimBoothRemote:FireServer()

    task.wait(2)

    -- Teleport player to owned booth
    TeleportRemote:FireServer()

    print("Booth claimed + teleported")
end

-- Do immediately on script run
autoClaim()

-- Reclaim after respawn
LocalPlayer.CharacterAdded:Connect(function()
    task.wait(3)
    autoClaim()
end)

-- =====================
-- AUTO JUMP ON DONATION
-- =====================
local raisedStat = LocalPlayer:WaitForChild("leaderstats"):WaitForChild("Raised")
local last = raisedStat.Value

raisedStat:GetPropertyChangedSignal("Value"):Connect(function()
    if raisedStat.Value > last then
        local char = LocalPlayer.Character
        local hum = char and char:FindFirstChild("Humanoid")

        if hum then
            for i = 1, 3 do
                hum:ChangeState(Enum.HumanoidStateType.Jumping)
                task.wait(0.35)
            end
        end
    end

    last = raisedStat.Value
end)

print("Loaded: Auto Booth + Jump + Anti-AFK")
